  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -e js,jsx --exec babel-node src/server",
    "prod:start": "node dist/server",
    "prod:build": "cross-env NODE_ENV=production rimraf dist/ && webpack && babel ./src --out-dir dist/ --ignore ./node_modules,./.eslintrc.json,./.babelrc,./npm-debug.log --copy-files",
    "dev:wds": "webpack-dev-server --progress",
    "test": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.13.4",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "mongoose": "^4.10.5",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "socket.io": "^2.0.3"
  }
